<div class="login-form-container">
    <!-- message Area -->
    <% if (message.length>0){%>
    <div class="alert alert-danger">
        <%=message%>
    </div>
    <% } %>
    <!-- login form -->
    <div class="row align-item-center justify-content-center">
        <div class="0ffert md-4 col-md-4">
            <form method="POST" autocomplete="" class="login-form">
                <fieldset class="form-group">
                    <label for="username">Username</label>
                    <input type="text" class="form-control" name="username" name="idå" placeholder="Username" autofocus>
                </fieldset>
                <fieldset class="form-group">
                    <label for="password">Password</label>
                    <input type="password" class="form-control" name="password" name="idå" placeholder="Password">
                </fieldset>
                <fieldset class="form-group text-right">
                    <input type="submit" class="btn login-form-submit">
                </fieldset>
                <fieldset class="form-group text-right login-form-register-wrapper">
                    Haven't had an account? <a href="/register" class="login-form-register">Register here</a>
                </fieldset>
            </form>
        </div>
    </div>
        
</div>